{
  "name": "boogie-solver",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "npm-run-all -s build:clean build:ts",
    "build:clean": "rimraf ./build/",
    "build:ts": "tsc",
    "demo": "ts-node demo.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "precommit": "npm-run-all -s lint test build",
    "test": "nyc --reporter=lcov --require ts-node/register tape tests.ts | faucet; nyc report ---reporter=text",
    "test:quick": "ts-node tests.ts"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts",
      "!src/**/*.test.ts",
      "!src/index.ts",
      "!src/types.ts"
    ],
    "exclude": [
      "build",
      "coverage",
      ".nyc_output",
      "node_modules"
    ],
    "extension": [
      ".ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "types": "./src/types/index.d.ts",
  "dependencies": {
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/tape": "^4.2.33",
    "faucet": "0.0.1",
    "glob": "^7.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "tape": "^4.9.2",
    "ts-node": "^7.0.1",
    "tslint-config-prettier": "^1.17.0"
  }
}
